  (:action ligar_esteira
    :parameters (esteira)
    :precondition
      (and
        (not
          (ligado esteira)
        )
      )
    :effect
      (and
        (ligado esteira)
      )
  )
  (:action extende_atuador
    :parameters (esteira atuador_simples1 inicio box1 item0 peqmet1)
    :precondition
      (and
        (type item0 peqmet1)
        (ligado esteira)
        (not
          (extended atuador_simples1)
        )
        (at item0 inicio)
        (not
          (blocked esteira)
        )
        (link atuador_simples1 box1)
      )
    :effect
      (and
        (at item0 box1)
        (not
          (at item0 inicio)
        )
        (extended atuador_simples1)
        (s_fimdecurso atuador_simples1)
        (blocked esteira)
      )
  )
  (:action retrai_atuador
    :parameters (esteira atuador_simples1)
    :precondition
      (and
        (s_fimdecurso atuador_simples1)
        (extended atuador_simples1)
        (blocked esteira)
      )
    :effect
      (and
        (not
          (extended atuador_simples1)
        )
        (not
          (s_fimdecurso atuador_simples1)
        )
        (not
          (blocked esteira)
        )
      )
  )
  (:action extende_atuador
    :parameters (esteira atuador_duplo1 inicio box3 item1 peqmet1)
